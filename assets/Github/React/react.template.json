{
  "name": "Node.js CI",
  "on": {
    "push": {
      "branches": [
        "main"
      ]
    },
    "pull_request": {
      "branches": [
        "main"
      ]
    }
  },

  "env":{
    "APP_LOCATION": "/",
    "API_LOCATION" : "api",
    "APP_ARTIFACT_LOCATION": "build",
    "AZURE_STATIC_WEB_APPS_API_TOKEN": "${{ secrets.AZURE_STATIC_WEB_APPS_API_TOKEN }}"
  },

  "jobs" : {}
}